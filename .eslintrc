{
  "env": {
    "node": true
  },
  "rules": {
    "brace-style": [1, "1tbs"],
    "camelcase": 0,
    "dot-notation": 1,
    "eol-last": 1,
    "eqeqeq": 1,
    "new-cap": 0,
    "no-alert": 1,
    "no-bitwise": 1,
    "no-empty": 1,
    "no-ex-assign": 1,
    "no-extra-semi": 1,
    "no-lonely-if": 1,
    "no-loop-func": 1,
    "no-multi-spaces": 1,
    "no-reserved-keys": 1,
    "no-trailing-spaces": 1,
    "no-undef": 1,
    "no-underscore-dangle": 1,
    "no-unused-vars": 1,
    "quotes": [1, "single", "avoid-escape"],
    "semi": 1,
    "space-after-keywords": 1,
    "space-before-blocks": 1,
    "space-infix-ops": 1,
    "strict": 0
  }
}
